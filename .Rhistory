poly_est <- function(y){
x <- 1:length(y)
lm(y ~ poly(x, 4, raw = TRUE))
}
exp_est <- function(y){
tryCatch({
x <- 1:length(y)
exp  <- lm(log(y) ~ x)
exp$coefficients[["x"]]
}, error = function(e){return(NaN)})
}
roll_exp_est <- rollify(exp_est, window= 7, unlist = TRUE)
theme_set(
theme_minimal() +
theme(legend.position = "right")
)
